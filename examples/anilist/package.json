{
  "name": "anilist-example",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:relay\" \"npm:dev:serve\"",
    "dev:relay": "relay-compiler --watch",
    "dev:serve": "tsx server.mts",
    "build": "relay-compiler && vite build --outDir dist/client && vite build --ssr src/tappedServerModule.tsx --outDir dist/server",
    "preview": "cross-env NODE_ENV=production node dist/server/tappedServerModule.js",
    "type-check": "relay-compiler && tsc --build --noEmit"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/http-proxy": "^11.3.0",
    "@fastify/middie": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "fastify": "^5.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-relay": "^20.1.1",
    "relay-runtime": "^20.1.1",
    "tapped": "workspace:*",
    "wouter": "^3.7.1"
  },
  "devDependencies": {
    "@stylexjs/dev-runtime": "^0.11.1",
    "@stylexjs/stylex": "^0.10.1",
    "@stylexswc/unplugin": "^0.6.5",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.9",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^19.0.2",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-relay": "^20.1.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.6.2",
    "relay-compiler": "^20.1.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^7.1.5",
    "vite-plugin-cjs-interop": "^2.3.0",
    "vite-plugin-relay": "^2.1.0"
  }
}
